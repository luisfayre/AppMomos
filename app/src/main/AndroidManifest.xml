<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tap.momos">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        </activity>

        <activity android:name=".Registros.LoginActivity"
                    android:theme="@style/AppThemeTecLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Registros.RegisterActivity" android:theme="@style/AppThemeTecLogin"/>
        <activity android:name=".PrincipalActivity" android:theme="@style/AppThemePrincipal"/>
        <activity android:name=".SubirImagen" android:theme="@style/AppThemePrincipal"/>
        <activity android:name=".ShowData" android:theme="@style/AppThemePrincipal"/>
    </application>

</manifest>
